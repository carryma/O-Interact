create table user_test(
id  number primary key,
nickname varchar2(100),
sex number(1),
country varchar2(20),
province varchar2(20),
city varchar2(20),
open_id varchar2(50),
head_img_url varchar2(100)
);
declare
begin

for i in 1:5 loop
	inser into user_test(i, nickname, sex, country,province,city,open_id,head_img_url) values
	(i,'yayma',2,'china','jiangsu','suzhou',null,null);
end loop;
commit;
end;